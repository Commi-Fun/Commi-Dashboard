    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/x-charts": "^8.5.3",
    "@mui/x-data-grid": "^8.5.3",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.81.5",
    "@types/bcryptjs": "^3.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookies-next": "^6.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.3",
    "jose": "^6.0.13",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
